<!-- volunteer-details-section start -->
<section class="volunteer-details-section p-t-130 p-b-120">
    <div class="container">
        <div class="row align-items-center" data-aos="fade-up" data-aos-duration="1000" data-aos-delay="600">
            <div class="col-xl-5">
                <div class="volunteer-detail-card">
                    <div class="thumb">
                        <a href='/volunteer'>
                            <img src="<%= data.image || '/assets/img/thumbs/thumb-83.webp' %>" alt="<%= data.name %>">
                        </a>
                        <div class="social-links">
                            <a href="<%= data.socials?.facebook || 'https://facebook.com' %>">
                                <i class="fab fa-facebook-f"></i>
                            </a>
                            <a href="<%= data.socials?.twitter || 'https://twitter.com' %>">
                                <i class="fab fa-x-twitter"></i>
                            </a>
                            <a href="<%= data.socials?.instagram || 'https://www.instagram.com' %>">
                                <i class="fab fa-instagram"></i>
                            </a>
                            <a href="<%= data.socials?.linkedin || 'https://linkedin.com' %>">
                                <i class="fa-brands fa-linkedin-in"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-6">
                <div class="volunteer-detail-content">
                    <div class="content-wrap">
                        <h4 class="subtitle">My Name is,</h4>
                        <h3 class="title"><%= data.name || 'BM Ashik (Moni)' %></h3>
                        <p><%= data.role || 'Senior Volunteer' %></p>
                    </div>
                    <div class="details-wrap">
                        <div class="info">
                            <p>Occupation:</p>
                            <span><%= data.occupation || 'Student' %></span>
                        </div>
                        <div class="info">
                            <p>Experience:</p>
                            <span><%= data.experience || '10 Years' %></span>
                        </div>
                        <div class="info">
                            <p>Email:</p>
                            <span><%= data.email || 'Roshua10@gmail.com' %></span>
                        </div>
                        <div class="info">
                            <p>Phone:</p>
                            <span><%= data.phone || '+855 (2669) 1234' %></span>
                        </div>
                    </div>
                    <div class="content-wrap">
                        <h3 class="title style-2">Summary</h3>
                        <p>
                            <%= data.summary || 'Hello Friend, My name is Bm Ashik (Moni). I am a Ecology specialists may be involved in a variety of tasks, including conducting field research, analyzing data, developing conservation strategies, or communicating scientific findings to the public or policymakers. They may also collaborate with other specialists, such as biologists, geologists, or environmental.' %>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- volunteer-details-section end -->
